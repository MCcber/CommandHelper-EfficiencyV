    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:AnimationComponents="clr-namespace:CBHK.CustomControl.AnimationComponents"
    xmlns:ColorPickers="clr-namespace:CBHK.CustomControl.ColorPickerComponents"
    xmlns:Container="clr-namespace:CBHK.CustomControl.Container"
    xmlns:CustomControl="clr-namespace:CBHK.CustomControl"
