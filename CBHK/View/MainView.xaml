            <CustomControl:IconTabItems
                Width="45"
                Height="45"
                Background="#2F2F2F"
                BorderThickness="0"
                Selectedbackground="#595959"
                Style="{DynamicResource IconTabItems}">
                <TabItem.HeaderTemplate>
                    <ItemContainerTemplate>
                        <Image
                            Width="35"
                            Height="35"
                            Source="/CBHK;component/Resource/CBHK/Image/IconUser.png" />
                    </ItemContainerTemplate>
                </TabItem.HeaderTemplate>
                <Grid Margin="5">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="50" />
                        <RowDefinition Height="*" />
                        <RowDefinition Height="50" />
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="50" />
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="*" />
                    </Grid.ColumnDefinitions>
                    <behaviors:Interaction.Triggers>
                        <behaviors:EventTrigger EventName="Loaded">
                            <behaviors:CallMethodAction MethodName="UserGrid_Loaded" TargetObject="{Binding}" />
                        </behaviors:EventTrigger>
                    </behaviors:Interaction.Triggers>
                    <Image
                        Grid.Row="0"
                        Grid.Column="0"
                        HorizontalAlignment="Left"
                        VerticalAlignment="Top"
                        Panel.ZIndex="1"
                        Cursor="Hand"
                        RenderOptions.BitmapScalingMode="HighQuality"
                        Source="{Binding UserHead}">
                        <Image.Clip>
                            <EllipseGeometry
                                Center="25,25"
                                RadiusX="30"
                                RadiusY="30" />
                        </Image.Clip>
                    </Image>
                    <Image
                        Grid.Row="0"
                        Grid.RowSpan="3"
                        Grid.Column="0"
                        Grid.ColumnSpan="3"
                        VerticalAlignment="Top"
                        Panel.ZIndex="0"
                        RenderOptions.BitmapScalingMode="HighQuality"
                        Source="{Binding UserBackground}"
                        Stretch="Uniform" />
                    <TextBlock
                        Grid.Row="0"
                        Grid.Column="1"
                        Margin="5,0"
                        HorizontalAlignment="Left"
                        VerticalAlignment="Center"
                        FontSize="30"
                        Foreground="White"
                        Text="{Binding UserID}"
                        TextAlignment="Left" />
                    <TextBlock
                        Grid.Row="0"
                        Grid.Column="2"
                        Margin="10,0,0,0"
                        HorizontalAlignment="Left"
                        VerticalAlignment="Center"
                        FontSize="30"
                        Foreground="White"
                        Text="{Binding UserName}" />
                    <TextBlock
                        Grid.Row="1"
                        Grid.Column="0"
                        Grid.ColumnSpan="3"
                        Margin="10,0,0,0"
                        HorizontalAlignment="Left"
                        VerticalAlignment="Top"
                        FontSize="25"
                        Foreground="White"
                        Text="{Binding UserDescription}"
                        TextTrimming="CharacterEllipsis" />
                </Grid>
            </CustomControl:IconTabItems>
        </TabControl>
    </Grid>
</localWindow:CommonWindow>
