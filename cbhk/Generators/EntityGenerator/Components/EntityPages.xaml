                x:Name="EnableButton"
                Grid.Column="5"
                Height="30"
                HorizontalAlignment="Left"
                VerticalAlignment="Top"
                BorderThickness="4,5,4,7"
                Foreground="White"
                IsChecked="{Binding Give, Mode=TwoWay}"
                SelectedToggleText="给予"
                Style="{DynamicResource RadiusToggleButton}"
                ToggleHeight="25"
                ToggleWidth="45"
                UnSelectedToggleText="召唤" />
            <CustomControls:StereoToggleButton
                HorizontalAlignment="Left"
                VerticalAlignment="Center"
                Background="#D77A34"
                BorderThickness="4,4,4,5"
                BottomBorderTexture="{StaticResource TextToggleButtonFalseBottomFrame}"
                Content="同步到文件"
                Foreground="Black"
                IsChecked="{Binding SyncToFile, Mode=OneWayToSource}"
                IsCheckedThickness="4"
                LeftBorderTexture="{StaticResource TextToggleButtonFalseLeftFrame}"
                RightBorderTexture="{StaticResource TextToggleButtonFalseRightFrame}"
                Style="{DynamicResource StereoToggleButton}"
                TopBorderTexture="{StaticResource TextToggleButtonFalseTopFrame}"
                TrueBottomBorderTexture="{StaticResource TextToggleButtonTrueBottomFrame}"
                TrueLeftBorderTexture="{StaticResource TextToggleButtonTrueLeftFrame}"
                TrueRightBorderTexture="{StaticResource TextToggleButtonTrueRightFrame}"
                TrueTopBorderTexture="{StaticResource TextToggleButtonTrueTopFrame}" />
            <CustomControls:StereoTextButton
                HorizontalAlignment="Left"
                VerticalAlignment="Center"
                Background="#EFF2F1"
                BorderThickness="3,1,3,5"
                BottomBorderTexture="{StaticResource ButtonNormalBottom}"
                Command="{Binding ClearUnnecessaryData}"
                Content="冻结数据"
                Foreground="Black"
                LeftBorderTexture="{StaticResource ButtonNormalLeft}"
                PressedBorderThickness="4"
                PressedBottomBorderTexture="{StaticResource ButtonPressedBottom}"
                PressedLeftBorderTexture="{StaticResource ButtonPressedLeft}"
                PressedRightBorderTexture="{StaticResource ButtonPressedRight}"
                PressedTopBorderTexture="{StaticResource ButtonPressedTop}"
                RightBorderTexture="{StaticResource ButtonNormalRight}"
                Style="{DynamicResource StereoTextButton}"
                ToolTipService.BetweenShowDelay="0"
                ToolTipService.InitialShowDelay="0"
                TopBorderTexture="{StaticResource ButtonNormalTop}">
                <CustomControls:StereoTextButton.ToolTip>
                    <ToolTip
                        Background="#2F2F2F"
                        Content="点击后清除历史实体的特指数据"
                        Foreground="White" />
                </CustomControls:StereoTextButton.ToolTip>
