<localWindow:CommonWindow
    x:Class="CBHK.View.Generators.ArmorStandView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:AnimationComponents="clr-namespace:CBHK.CustomControls.AnimationComponents"
    xmlns:ColorPickers="clr-namespace:CBHK.CustomControls.ColorPickerComponents"
    xmlns:CustomControls="clr-namespace:CBHK.CustomControls"
    xmlns:GeneralTools="clr-namespace:CBHK.GeneralTools"
    xmlns:behaviors="http://schemas.microsoft.com/xaml/behaviors"
    xmlns:control_datacontexts="clr-namespace:CBHK.ControlsDataContexts"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:hx="http://helix-toolkit.org/wpf"
    xmlns:localWindow="clr-namespace:CBHK.WindowDictionaries"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:prism="http://prismlibrary.com/"
    Title="盔甲架生成器"
    Width="1080"
    Height="840"
    MinWidth="1080"
    MinHeight="840"
    prism:ViewModelLocator.AutoWireViewModel="True"
    Background="#37383B"
    Icon="/CBHK;component/Resource/Common/Image/SpawnerIcon/IconArmorStand.png"
    ResizeMode="CanResizeWithGrip"
    WindowStartupLocation="CenterScreen"
    mc:Ignorable="d">

    <Window.InputBindings>
        <KeyBinding Key="F5" Command="{Binding RunCommand}" />
        <KeyBinding
            Key="Esc"
            Command="{Binding ReturnCommand}"
            CommandParameter="{Binding RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=Window}}" />
    </Window.InputBindings>

    <Window.Resources>
        <ImageBrush x:Key="ButtonNormal" ImageSource="/CBHK;component/Resource/Common/Image/ButtonNormal.png" />
        <ImageBrush x:Key="ButtonPressed" ImageSource="/CBHK;component/Resource/Common/Image/ButtonPressed.png" />
        <ImageBrush x:Key="IssueIcon" ImageSource="/CBHK;component/Resource/Common/Image/issue.png" />
        <ImageBrush x:Key="TextBoxBackground" ImageSource="/CBHK;component/Resource/Common/Image/TextEditBox.png" />
        <ImageBrush x:Key="DataButtonBackground" ImageSource="/CBHK;component/Resource/Common/Image/TextToggleButtonNormal.png" />
        <ImageBrush x:Key="DataButtonPressedBackground" ImageSource="/CBHK;component/Resource/Common/Image/TextToggleButtonPressed.png" />
        <ImageBrush x:Key="LeftArrow" ImageSource="/CBHK;component/Resource/Common/Image/IconArrowLeft.png" />
        <ImageBrush x:Key="RightArrow" ImageSource="/CBHK;component/Resource/Common/Image/IconArrowRight.png" />
        <BitmapImage x:Key="Slot" UriSource="/CBHK;component/Resource/Common/Image/Slot.png" />

        <BitmapImage x:Key="SmithingTemplate" UriSource="/CBHK;component/Resource/Common/Image/SmithingTemplate.png" />
        <BitmapImage x:Key="TrimMaterial" UriSource="/CBHK;component/Resource/Common/Image/TrimMaterial.png" />

        <control_datacontexts:DoubleToFloat x:Key="DoubleToFloatConverter" />

        <GeneralTools:PositionConverter x:Key="PositionConverter" />
    </Window.Resources>
    <Grid Background="#7F323232">
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
            <RowDefinition Height="*" />
            <RowDefinition Height="Auto" />
        </Grid.RowDefinitions>
        <DockPanel
            Grid.Row="0"
            Height="60"
            VerticalAlignment="Top">
            <CustomControls:IconTextButtons
                Width="25"
                Height="25"
                HorizontalContentAlignment="Center"
                VerticalContentAlignment="Center"
                Background="{StaticResource LeftArrow}"
                BorderThickness="0"
                Command="{Binding ReturnCommand}"
                CommandParameter="{Binding RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=Window}}"
                FontSize="15"
                Style="{DynamicResource IconTextButton}"
                ToolTipService.BetweenShowDelay="0"
                ToolTipService.InitialShowDelay="0">
                <CustomControls:IconTextButtons.ToolTip>
                    <ToolTip
                        Background="#484848"
                        Content="ESC:返回主页"
                        Foreground="White" />
                </CustomControls:IconTextButtons.ToolTip>
            </CustomControls:IconTextButtons>
            <CustomControls:IconTextButtons
                Width="25"
                Height="25"
                HorizontalContentAlignment="Center"
                VerticalContentAlignment="Center"
                Background="{StaticResource RightArrow}"
                BorderThickness="0"
                Command="{Binding RunCommand}"
                FontSize="15"
                Style="{DynamicResource IconTextButton}"
                ToolTipService.BetweenShowDelay="0"
                ToolTipService.InitialShowDelay="0">
                <CustomControls:IconTextButtons.ToolTip>
                    <ToolTip
                        Background="#484848"
                        Content="F5:运行"
                        Foreground="White" />
                </CustomControls:IconTextButtons.ToolTip>
            </CustomControls:IconTextButtons>
            <TextBlock
                Margin="5,0"
                VerticalAlignment="Center"
                FontSize="15"
                Foreground="White"
                Text="版本"
                TextAlignment="Center" />
            <CustomControls:TextComboBoxs
                Width="110"
                Height="30"
                HorizontalAlignment="Left"
                IsReadOnly="True"
                ItemsSource="{Binding VersionSource}"
                SelectedIndex="0"
                SelectedItem="{Binding SelectedVersion, Mode=TwoWay}"
                Style="{DynamicResource TextComboBoxStyle}">
                <behaviors:Interaction.Triggers>
                    <behaviors:EventTrigger EventName="SelectionChanged">
                        <behaviors:CallMethodAction MethodName="Version_SelectionChanged" TargetObject="{Binding}" />
                    </behaviors:EventTrigger>
                </behaviors:Interaction.Triggers>
            </CustomControls:TextComboBoxs>
            <TextBlock
                Margin="5,5,5,0"
                HorizontalAlignment="Left"
                VerticalAlignment="Center"
                FontSize="15"
                Foreground="White"
                Text="名称" />
            <CustomControls:StylizedTextBox
                Width="140"
                Height="80"
                VerticalAlignment="Center"
                VerticalContentAlignment="Center"
                BorderBrush="Black"
                Foreground="White">
                <behaviors:Interaction.Triggers>
                    <behaviors:EventTrigger EventName="Loaded">
                        <behaviors:CallMethodAction MethodName="CustomNameBox_Loaded" TargetObject="{Binding}" />
                    </behaviors:EventTrigger>
                    <behaviors:EventTrigger EventName="PreviewKeyUp">
                        <behaviors:CallMethodAction MethodName="StylizedTextBox_PreviewKeyUp" TargetObject="{Binding}" />
                    </behaviors:EventTrigger>
                </behaviors:Interaction.Triggers>
            </CustomControls:StylizedTextBox>
            <CustomControls:RadiusToggleButtons
                Margin="5,0"
                HorizontalAlignment="Left"
                VerticalAlignment="Center"
                FontSize="12"
                Foreground="White"
                IsChecked="{Binding Path=CustomNameVisible, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                SelectedToggleText="名称可见性"
                Style="{StaticResource RadiusToggleButton}"
                ToggleHeight="26"
                ToggleWidth="46"
                UnSelectedToggleText="名称可见性" />
            <CustomControls:RadiusToggleButtons
                Margin="5,0,0,0"
                HorizontalAlignment="Left"
                VerticalAlignment="Center"
                Foreground="White"
                IsChecked="{Binding ShowGeneratorResult, Mode=OneWayToSource}"
                SelectedToggleText="显示结果"
                Style="{DynamicResource RadiusToggleButton}"
                ToggleHeight="26"
                ToggleWidth="46"
                UnSelectedToggleText="隐藏结果" />
        </DockPanel>
        <Grid
            Grid.Row="1"
            HorizontalAlignment="Stretch"
            Background="Transparent">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto" />
                <ColumnDefinition Width="*" />
                <ColumnDefinition Width="*" />
            </Grid.ColumnDefinitions>
            <Grid
                Grid.Column="0"
                Width="550"
                Margin="0,5,0,0"
                Background="Transparent">
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="*" />
                </Grid.RowDefinitions>
                <StackPanel Grid.Row="0">
                    <DockPanel Grid.Row="0" Margin="10,0,0,5">
                        <TextBlock
                            Margin="10,0,10,0"
                            FontSize="20"
                            FontWeight="Bold"
                            Foreground="White"
                            Text="动作与权限" />
                        <CustomControls:RadiusToggleButtons
                            Width="45"
                            Height="25"
                            HorizontalAlignment="Left"
                            IsChecked="{Binding Path=CanResetAllPose, Mode=TwoWay}"
                            Style="{DynamicResource RadiusToggleButton}"
                            ToggleHeight="25"
                            ToggleWidth="45" />
                        <CustomControls:IconTextButtons
                            Width="35"
                            Height="25"
                            Margin="5,0,0,0"
                            HorizontalAlignment="Left"
                            Background="{StaticResource ButtonNormal}"
                            Command="{Binding ResetAllPoseCommand}"
                            Content="重置"
                            Foreground="{Binding Path=ResetAllPoseButtonForeground, Mode=OneWay, UpdateSourceTrigger=PropertyChanged}"
                            PressedBackground="{StaticResource ButtonPressed}"
                            Style="{DynamicResource IconTextButton}" />
