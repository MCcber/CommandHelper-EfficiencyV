                                        HorizontalAlignment="Left"
                                        VerticalAlignment="Center"
                                        Foreground="White"
                                        Text="Y轴"
                                        TextAlignment="Center" />
                                    <CustomControls:ColorSliders
                                        Width="150"
                                        Height="30"
                                        Margin="0,0,0,0"
                                        HorizontalAlignment="Left"
                                        VerticalAlignment="Center"
                                        BackgroundColor="Gray"
                                        BackgroundHeight="10"
                                        BackgroundWidth="140"
                                        BorderThickness="0"
                                        Foreground="Black"
                                        HanderHeight="25"
                                        HanderWidth="25"
                                        Maximum="180"
                                        Minimum="-180"
                                        Style="{DynamicResource ColorSlider}"
                                        Value="{Binding Path=HeadY, Mode=TwoWay}" />
                                    <Slider
                                        Width="120"
                                        Foreground="White"
                                        Maximum="180"
                                        Minimum="-180"
                                        Style="{DynamicResource NumberBoxStyle}"
                                        Value="{Binding Path=HeadY, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />
                                    <CustomControls:RadiusToggleButtons
                                        Height="25"
                                        Foreground="White"
                                        IsChecked="{Binding CannotTakeOrReplaceHead, Mode=TwoWay}"
                                        SelectedToggleText="防止移除或改变"
                                        Style="{DynamicResource RadiusToggleButton}"
                                        ToggleHeight="25"
                                        ToggleWidth="45"
                                        UnSelectedToggleText="防止移除或改变" />
                                </DockPanel>
                                <DockPanel
                                    Grid.Row="3"
                                    Height="30"
                                    Margin="10,0,0,0">
                                    <TextBlock
                                        HorizontalAlignment="Left"
                                        VerticalAlignment="Center"
                                        Foreground="White"
                                        Text="Z轴"
                                        TextAlignment="Center" />
                                    <CustomControls:ColorSliders
                                        Width="150"
                                        Height="30"
                                        Margin="0,0,0,0"
                                        HorizontalAlignment="Left"
                                        VerticalAlignment="Center"
                                        BackgroundColor="Gray"
                                        BackgroundHeight="10"
                                        BackgroundWidth="140"
                                        BorderThickness="0"
                                        Foreground="Black"
                                        HanderHeight="25"
                                        HanderWidth="25"
