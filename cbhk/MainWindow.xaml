    <Window.Resources>
        <ContextMenu
            x:Key="SysTrayMenu"
            BorderBrush="#424242"
            BorderThickness="1">
            <MenuItem
                Background="#2F2F2F"
                BorderBrush="Transparent"
                BorderThickness="1"
                Click="ShowWindowCommand"
                Foreground="White"
                Header="打开管家" />
            <MenuItem
                Background="#2F2F2F"
                BorderBrush="Transparent"
                BorderThickness="1"
                Click="ExitApplicationCommand"
                Foreground="White"
                Header="退出" />
        </ContextMenu>

        <VisualBrush
            x:Key="BackgroundBrush"
            AlignmentX="Center"
            AlignmentY="Center"
            Stretch="None">
            <VisualBrush.Visual>
                <Grid>
                    <TextBlock
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        Background="Transparent"
                        FontSize="12"
                        Foreground="#D6D6D6"
                        Text="正以开发者模式运行"
                        TextAlignment="Center" />
                </Grid>
            </VisualBrush.Visual>
        </VisualBrush>

    </Window.Resources>

