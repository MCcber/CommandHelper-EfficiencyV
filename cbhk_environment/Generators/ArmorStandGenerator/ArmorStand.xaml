                        Style="{DynamicResource IconTabItems}">
                        <Grid Background="#515B59">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="*" />
                                <RowDefinition Height="*" />
                                <RowDefinition Height="*" />
                                <RowDefinition Height="*" />
                            </Grid.RowDefinitions>
                            <DockPanel Grid.Row="0" Margin="25,0,0,5">
                                <TextBlock
