<local_window:CommonWindow
    x:Class="cbhk_environment.Generators.ArmorStandGenerator.ArmorStand"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:ColorPickers="clr-namespace:cbhk_environment.CustomControls.ColorPickers"
    xmlns:CustomControls="clr-namespace:cbhk_environment.CustomControls"
    xmlns:behaviors="http://schemas.microsoft.com/xaml/behaviors"
    xmlns:control_datacontexts="clr-namespace:cbhk_environment.ControlsDataContexts"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:hx="http://helix-toolkit.org/wpf"
    xmlns:local_datacontext="clr-namespace:cbhk_environment.Generators.ArmorStandGenerator"
    xmlns:local_window="clr-namespace:cbhk_environment.WindowDictionaries"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Title="盔甲架生成器"
    Width="900"
    Height="700"
    MinWidth="900"
    MinHeight="675"
    Background="#37383B"
    Icon="/cbhk_environment;component/resources/common/images/spawnerIcons/IconArmorStand.png"
    ResizeMode="CanMinimize"
    WindowStartupLocation="CenterScreen"
    WindowStyle="None"
    mc:Ignorable="d">
    <Window.DataContext>
        <local_datacontext:armor_stand_datacontext />
    </Window.DataContext>

    <Window.InputBindings>
        <KeyBinding Key="F5" Command="{Binding RunCommand}" />
        <KeyBinding
            Key="Esc"
            Command="{Binding ReturnCommand}"
            CommandParameter="{Binding RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=Window}}" />
    </Window.InputBindings>

    <Window.Resources>
        <ImageBrush x:Key="ButtonNormal" ImageSource="/cbhk_environment;component/resources/common/images/ButtonNormal.png" />
        <ImageBrush x:Key="ButtonPressed" ImageSource="/cbhk_environment;component/resources/common/images/ButtonPressed.png" />
        <ImageBrush x:Key="IssueIcon" ImageSource="/cbhk_environment;component/resources/common/images/issue.png" />
        <ImageBrush x:Key="TextBoxBackground" ImageSource="/cbhk_environment;component/resources/common/images/TextEditBox.png" />
        <ImageBrush x:Key="DataButtonBackground" ImageSource="/cbhk_environment;component/resources/common/images/TextToggleButtonNormal.png" />
        <ImageBrush x:Key="DataButtonPressedBackground" ImageSource="/cbhk_environment;component/resources/common/images/TextToggleButtonPressed.png" />
        <ImageBrush x:Key="LeftArrow" ImageSource="/cbhk_environment;component/resources/common/images/IconArrowLeft.png" />
        <ImageBrush x:Key="RightArrow" ImageSource="/cbhk_environment;component/resources/common/images/IconArrowRight.png" />

        <control_datacontexts:DoubleToFloat x:Key="DoubleToFloatConverter" />
    </Window.Resources>
    <StackPanel VerticalAlignment="Stretch" Background="#7F323232">
        <DockPanel Height="30">
            <CustomControls:IconTextButtons
                Width="25"
                Height="25"
                HorizontalContentAlignment="Center"
                VerticalContentAlignment="Center"
                Background="{StaticResource LeftArrow}"
                BorderThickness="0"
                Command="{Binding ReturnCommand}"
                CommandParameter="{Binding RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=Window}}"
                FontSize="15"
                Style="{DynamicResource IconTextButton}"
                ToolTipService.BetweenShowDelay="0"
                ToolTipService.InitialShowDelay="0">
                <CustomControls:IconTextButtons.ToolTip>
                    <ToolTip
                        Background="#484848"
                        Content="ESC:返回主页"
                        Foreground="White" />
                </CustomControls:IconTextButtons.ToolTip>
            </CustomControls:IconTextButtons>
            <CustomControls:IconTextButtons
                Width="25"
                Height="25"
                HorizontalContentAlignment="Center"
                VerticalContentAlignment="Center"
                Background="{StaticResource RightArrow}"
                BorderThickness="0"
                Command="{Binding RunCommand}"
                FontSize="15"
                Style="{DynamicResource IconTextButton}"
                ToolTipService.BetweenShowDelay="0"
                ToolTipService.InitialShowDelay="0">
                <CustomControls:IconTextButtons.ToolTip>
                    <ToolTip
                        Background="#484848"
                        Content="F5:运行"
                        Foreground="White" />
                </CustomControls:IconTextButtons.ToolTip>
            </CustomControls:IconTextButtons>
            <TextBlock
                Margin="10,0"
                VerticalAlignment="Center"
                FontSize="15"
                Foreground="White"
                Text="版本"
                TextAlignment="Center" />
            <CustomControls:TextComboBoxs
                Width="100"
                HorizontalAlignment="Left"
                FontSize="20"
                ItemsSource="{Binding VersionSource}"
                SelectedIndex="0"
                SelectedItem="{Binding SelectedVersion, Mode=TwoWay}"
                Style="{DynamicResource TextComboBoxStyle}" />
            <CustomControls:RadiusToggleButtons
                Margin="5,0,0,0"
                HorizontalAlignment="Left"
                Foreground="White"
                IsChecked="{Binding ShowGeneratorResult, Mode=OneWayToSource}"
                SelectedToggleText="显示结果"
                Style="{DynamicResource RadiusToggleButton}"
                ToggleHeight="25"
                ToggleWidth="45"
                UnSelectedToggleText="隐藏结果" />
        </DockPanel>
        <DockPanel
            MinHeight="570"
            HorizontalAlignment="Stretch"
            Background="Transparent"
            FlowDirection="LeftToRight"
            LastChildFill="True">
            <StackPanel
                Width="550"
                Margin="0,5,0,0"
                Background="Transparent">
                <DockPanel Margin="10,0,0,0">
                    <TextBlock
                        Margin="5,0,0,0"
                        VerticalAlignment="Center"
                        FontSize="15"
                        Foreground="White"
                        Text="标签" />
                    <TextBox
                        Width="100"
                        Margin="5,0"
                        Padding="5,5,0,0"
                        BorderBrush="Black"
                        CaretBrush="White"
                        FontSize="20"
                        Foreground="White"
                        Text="{Binding Path=Tag, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />
                    <TextBlock
                        Width="40"
                        Margin="5,0"
                        HorizontalAlignment="Left"
                        VerticalAlignment="Center"
                        FontSize="15"
                        Foreground="White"
                        Text="名称" />
                    <TextBox
                        Width="100"
                        Padding="5,5,0,0"
                        HorizontalAlignment="Left"
                        BorderBrush="Black"
                        CaretBrush="White"
                        FontSize="20"
                        Foreground="White"
                        Text="{Binding Path=CustomName, Mode=OneWayToSource, UpdateSourceTrigger=PropertyChanged}" />
                    <CustomControls:RadiusToggleButtons
                        Margin="5,0"
                        HorizontalAlignment="Left"
                        FontSize="12"
                        Foreground="White"
                        IsChecked="{Binding Path=CustomNameVisible, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                        SelectedToggleText="名称可见性"
                        Style="{StaticResource RadiusToggleButton}"
                        ToggleHeight="26"
                        ToggleWidth="45"
                        UnSelectedToggleText="名称可见性" />
                    <TextBlock
                        Margin="5,0,0,0"
                        VerticalAlignment="Center"
                        FontSize="15"
                        Foreground="White"
                        Text="名称颜色" />
                    <ColorPickers:ColorPickers
                        Width="35"
                        Margin="5,0"
                        Background="Black"
                        SelectColor="{Binding CustomNameColor}" />
                </DockPanel>
                <TextBlock
                    Margin="10,5,0,2"
                    FontSize="20"
                    FontWeight="Bold"
                    Foreground="White"
                    Text="动作与权限" />
                <DockPanel Margin="10,0,0,5">
                    <CustomControls:RadiusToggleButtons
                        Width="45"
                        Height="25"
                        HorizontalAlignment="Left"
                        IsChecked="{Binding Path=CanResetAllPose, Mode=TwoWay}"
                        Style="{DynamicResource RadiusToggleButton}"
                        ToggleHeight="25"
                        ToggleWidth="45" />
                    <CustomControls:StereoTextButton
                        Width="90"
                        Height="30"
                        Margin="5,0,0,0"
                        HorizontalAlignment="Left"
                        Background="#EFF2F1"
                        BorderThickness="4,3.5,4,7.2"
                        BottomBorderTexture="{DynamicResource ButtonNormalBottom}"
                        ClickMode="Press"
                        Command="{Binding ResetAllPoseCommand}"
                        Content="重置所有动作"
                        Foreground="{Binding Path=ResetAllPoseButtonForeground, Mode=OneWay, UpdateSourceTrigger=PropertyChanged}"
                        LeftBorderTexture="{DynamicResource ButtonNormalLeft}"
                        PressedBorderThickness="4 4 4 4"
                        PressedBottomBorderTexture="{DynamicResource ButtonPressedBottom}"
                        PressedLeftBorderTexture="{DynamicResource ButtonPressedLeft}"
                        PressedRightBorderTexture="{DynamicResource ButtonPressedRight}"
                        PressedTopBorderTexture="{DynamicResource ButtonPressedTop}"
                        RightBorderTexture="{DynamicResource ButtonNormalRight}"
                        Style="{DynamicResource StereoTextButton}"
                        TopBorderTexture="{DynamicResource ButtonNormalTop}" />
                    <CustomControls:IconTextButtons
                        Width="18"
                        Height="18"
                        Margin="5,0"
                        HorizontalAlignment="Left"
                        Background="{StaticResource IssueIcon}"
                        Style="{DynamicResource IconTextButton}"
                        ToolTipService.BetweenShowDelay="0"
                        ToolTipService.InitialShowDelay="0">
                        <CustomControls:IconTextButtons.ToolTip>
                            <ToolTip
                                Background="#424242"
                                Content="1.左键进入/退出三轴合一编辑模式 2.横向控制X轴 3.纵向控制Z轴 4.滚轮控制Y轴"
                                Foreground="White" />
                        </CustomControls:IconTextButtons.ToolTip>
                    </CustomControls:IconTextButtons>
                </DockPanel>
                <TabControl
                    Height="150"
                    Margin="6,0,0,0"
                    Background="Transparent"
                    BorderBrush="Black"
                    BorderThickness="0"
                    ScrollViewer.VerticalScrollBarVisibility="Auto"
                    SelectedIndex="0"
                    Style="{DynamicResource TabControlWithScrollBar}"
                    TabStripPlacement="Left">
                    <CustomControls:IconTabItems
                        Width="50"
                        Height="30"
                        Background="{StaticResource DataButtonBackground}"
                        FontSize="15"
                        Foreground="White"
                        HeaderText="头部"
                        Selectedbackground="{StaticResource DataButtonPressedBackground}"
                        Style="{DynamicResource IconTabItems}">
