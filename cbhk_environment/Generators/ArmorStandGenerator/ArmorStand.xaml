                                        <behaviors:EventTrigger EventName="MouseLeave">
                                            <behaviors:CallMethodAction MethodName="ThreeAxisMouseLeave" TargetObject="{Binding}" />
                                        </behaviors:EventTrigger>
                                        <behaviors:EventTrigger EventName="MouseMove">
                                            <behaviors:CallMethodAction MethodName="ThreeAxisMouseMove" TargetObject="{Binding}" />
                                        </behaviors:EventTrigger>
                                        <behaviors:EventTrigger EventName="MouseWheel">
                                            <behaviors:CallMethodAction MethodName="ThreeAxisMouseWheel" TargetObject="{Binding}" />
                                        </behaviors:EventTrigger>
                                    </behaviors:Interaction.Triggers>
                                    <Button.Content>
                                        <Viewbox
                                            Width="15"
                                            Height="15"
                                            Margin="0,0,0,2">
                                            <Path Fill="Black">
                                                <Path.Data>
                                                    M980.949333 861.525333c0-0.256 0.042667-0.469333 0-0.682667l-49.834667-177.152c-2.304-8.661333-7.936-15.616-15.146667-19.797333-7.168-4.224-13.098667-10.581333-21.717333-8.277333-17.408 4.693333-27.733333 22.442667-23.04 39.978667l26.496 106.282667-356.053333-205.568L541.653333 185.941333l78.848 76.16c12.757333 12.757333 33.28 12.757333 46.037333 0 6.314667-6.314667 3.754667-14.592 3.754667-22.997333 0-8.362667-3.242667-16.64-9.557333-22.954667l-128.64-131.797333c-0.085333-0.128-0.256-0.170667-0.512-0.298667-2.816-2.858667-9.514667-5.162667-13.226667-6.741333-3.413333-1.365333-0.597333-1.877333-4.096-2.133333-0.725333 0-1.365333-0.341333-2.048-0.341333-0.512 0-0.853333 0.256-1.322667 0.256-3.797333 0.170667-5.674667 0.853333-9.344 2.218667-3.2 1.365333-5.930667 3.456-8.490667 5.674667-0.682667 0.512-1.450667 0.768-2.048 1.365333L359.381333 216.192c-12.8 12.672-6.912 33.322667 5.802667 45.952 12.714667 12.757333 33.322667 12.757333 45.994667 0l70.357333-76.16 0.042667 414.336-355.370667 205.141333 26.581333-106.325333c4.693333-17.450667-5.632-35.2-23.04-39.936-8.661333-2.304-14.506667 4.053333-21.76 8.234667-7.210667 4.181333-12.842667 11.136-15.146667 19.797333l-49.792 177.237333c-0.085333 0.170667 0 0.341333 0 0.597333-1.024 3.968 0.256 10.794667 0.725333 14.848 0.512 3.626667-1.322667 1.493333 0.256 4.522667 0.341333 0.725333 0.341333 1.408 0.682667 2.005333 0.256 0.469333 0.682667 0.682667 0.896 1.024 2.048 3.157333 3.626667 4.437333 6.528 6.954667 2.773333 2.090667 6.016 3.370667 9.258667 4.522667 0.725333 0.298667 1.408 0.896 2.218667 1.066667l179.968 48.213333c17.365333 4.693333 32.298667-10.666667 36.821333-28.074667 4.693333-17.322667-5.589333-35.242667-22.954667-39.808l-101.248-22.869333 358.784-207.146667 352.682667 203.605333-101.205333 22.869333c-17.322667 4.565333-27.605333 22.442667-22.954667 39.808 4.522667 17.408 19.498667 32.810667 36.821333 28.074667l180.010667-48.213333c0.853333-0.213333 1.493333