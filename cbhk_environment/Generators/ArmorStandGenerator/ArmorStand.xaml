                                                </Path.Data>
                                            </Path>
                                        </Viewbox>
                                    </Button.Content>
                                </CustomControls:IconTextButtons>
                                <DockPanel Margin="0,0,5,0" HorizontalAlignment="Right">
                                    <CustomControls:RadiusToggleButtons
                                        Width="45"
                                        Height="25"
                                        Margin="5,5,0,0"
                                        HorizontalAlignment="Left"
                                        VerticalAlignment="Top"
                                        IsChecked="{Binding Path=CanResetHeadPose, Mode=TwoWay}"
                                        Style="{StaticResource RadiusToggleButton}"
                                        ToggleHeight="25"
                                        ToggleWidth="45" />
                                    <CustomControls:IconTextButtons
                                        Width="60"
                                        Height="30"
                                        Margin="0,2,0,0"
                                        HorizontalAlignment="Left"
                                        VerticalAlignment="Top"
                                        Background="{StaticResource ButtonNormal}"
                                        ClickMode="Press"
                                        Command="{Binding ResetHeadPoseCommand}"
                                        Content="重置"
                                        Foreground="{Binding Path=ResetHeadPoseButtonForeground, Mode=OneWay}"
                                        PressedBackground="{StaticResource ButtonPressed}"
                                        Style="{DynamicResource IconTextButton}" />
                                </DockPanel>
                            </DockPanel>
                            <DockPanel
                                Grid.Row="1"
                                Height="30"
                                Margin="10,0,0,0">
                                <TextBlock
                                    Margin="0,0,-1,0"
                                    HorizontalAlignment="Left"
                                    VerticalAlignment="Center"
                                    Foreground="White"
                                    Text="X轴"
                                    TextAlignment="Center" />
                                <CustomControls:ColorSliders
                                    Width="150"
                                    Height="31"
                                    HorizontalAlignment="Left"
                                    VerticalAlignment="Center"
                                    BackgroundColor="Gray"
                                    BackgroundHeight="10"
                                    BackgroundWidth="140"
                                    BorderThickness="0"
                                    Foreground="Black"
                                    HanderHeight="25"
                                    HanderWidth="25"
                                    Maximum="180"
                                    Minimum="-180"
                                    Style="{DynamicResource ColorSlider}"
                                    Value="{Binding Path=HeadX, Mode=TwoWay}" />
                                <Slider
                                    Width="120"
                                    Foreground="White"
                                    Maximum="180"
                                    Minimum="-180"
                                    Style="{DynamicResource NumberBoxStyle}"
                                    Value="{Binding Path=HeadX, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />
                                <CustomControls:RadiusToggleButtons
                                    Height="25"
                                    Margin="0,2"
                                    HorizontalAlignment="Left"
                                    Foreground="White"
                                    IsChecked="{Binding CannotPlaceOrReplaceHead, Mode=TwoWay}"
                                    SelectedToggleText="防止添加或改变"
                                    Style="{DynamicResource RadiusToggleButton}"
                                    ToggleHeight="25"
                                    ToggleWidth="45"
                                    UnSelectedToggleText="防止添加或改变" />
                            </DockPanel>
                            <DockPanel
                                Grid.Row="2"
                                Height="30"
                                Margin="10,0,0,0">
                                <TextBlock
                                    HorizontalAlignment="Left"
                                    VerticalAlignment="Center"
                                    Foreground="White"
                                    Text="Y轴"
                                    TextAlignment="Center" />
                                <CustomControls:ColorSliders
                                    Width="150"
                                    Height="30"
                                    Margin="0,0,0,0"
                                    HorizontalAlignment="Left"
                                    VerticalAlignment="Center"
                                    BackgroundColor="Gray"
                                    BackgroundHeight="10"
                                    BackgroundWidth="140"
                                    BorderThickness="0"
                                    Foreground="Black"
                                    HanderHeight="25"
                                    HanderWidth="25"
                                    Maximum="180"
                                    Minimum="-180"
                                    Style="{DynamicResource ColorSlider}"
                                    Value="{Binding Path=HeadY, Mode=TwoWay}" />
                                <Slider
                                    Width="120"
                                    Foreground="White"
                                    Maximum="180"
                                    Minimum="-180"
                                    Style="{DynamicResource NumberBoxStyle}"
                                    Value="{Binding Path=HeadY, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />
                                <CustomControls:RadiusToggleButtons
                                    Height="25"
                                    Margin="0,0"
                                    Background="#FF191919"
                                    Foreground="White"
                                    IsChecked="{Binding CannotTakeOrReplaceHead, Mode=TwoWay}"
                                    SelectedToggleText="防止移除或改变"
                                    Style="{DynamicResource RadiusToggleButton}"
                                    ToggleHeight="25"
                                    ToggleWidth="45"
                                    UnSelectedToggleText="防止移除或改变" />
                            </DockPanel>
                            <DockPanel
                                Grid.Row="3"
                                Height="30"
                                Margin="10,0,0,0">
                                <TextBlock
                                    HorizontalAlignment="Left"
                                    VerticalAlignment="Center"
                                    Foreground="White"
                                    Text="Z轴"
                                    TextAlignment="Center" />
                                <CustomControls:ColorSliders
                                    Width="150"
                                    Height="30"
                                    Margin="0,0,0,0"
                                    HorizontalAlignment="Left"
                                    VerticalAlignment="Center"
                                    BackgroundColor="Gray"
                                    BackgroundHeight="10"
                                    BackgroundWidth="140"
                                    BorderThickness="0"
                                    Foreground="Black"
                                    HanderHeight="25"
                                    HanderWidth="25"
                                    Maximum="180"
                                    Minimum="-180"
                                    Style="{DynamicResource ColorSlider}"
                                    Value="{Binding Path=HeadZ, Mode=TwoWay}" />
                                <Slider
                                    Width="120"
                                    Foreground="White"
                                    Maximum="180"
                                    Minimum="-180"
                                    Style="{DynamicResource NumberBoxStyle}"
                                    Value="{Binding Path=HeadZ, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />
                                <CustomControls:RadiusToggleButtons
                                    Height="25"
                                    Margin="0,2"
                                    Background="#FF191919"
                                    Foreground="White"
