<local_window:CommonWindow
    x:Class="cbhk_environment.More.ConversationGroup"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:cbhk_environment.More"
    xmlns:local_window="clr-namespace:cbhk_environment.WindowDictionaries"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Title="加入QQ群一起讨论"
    Width="400"
    Height="500"
    MinWidth="400"
    MinHeight="500"
    ResizeMode="CanResizeWithGrip"
    WindowStartupLocation="CenterScreen"
    WindowStyle="None"
    mc:Ignorable="d">
    <Image
        Cursor="Hand"
        MouseLeftButtonUp="ClickToJoinGroup"
        Source="/cbhk_environment;component/resources/cbhk_form/images/more/conversation_group.png"
        Stretch="Uniform" />
</local_window:CommonWindow>
