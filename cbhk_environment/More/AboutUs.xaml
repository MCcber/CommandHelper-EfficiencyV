    <ScrollViewer
        Width="680"
        HorizontalAlignment="Center"
        VerticalAlignment="Top"
        HorizontalScrollBarVisibility="Disabled"
        VerticalScrollBarVisibility="Disabled">
        <Viewbox>
            <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
                <Image
                    Width="500"
                    Margin="0,5,0,0"
                    HorizontalAlignment="Center"
                    VerticalAlignment="Top"
                    RenderOptions.BitmapScalingMode="HighQuality"
                    Source="/cbhk_environment;component/resources/cbhk_form/images/more/thanks/Title.png"
                    Stretch="Uniform" />
                <DockPanel
                    Margin="0,10,0,0"
                    HorizontalAlignment="Center"
                    VerticalAlignment="Top"
                    LastChildFill="False">
                    <CustomControls:IconTextButtons
                        Width="200"
                        Height="136"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        Background="{StaticResource 1.png}"
                        Command="{Binding LinkCommand}"
                        CommandParameter="{Binding RelativeSource={RelativeSource Mode=Self}}"
                        NeedMouseOverStyle="True"
                        PressedBackground="{StaticResource 1Hover.png}"
                        Style="{DynamicResource IconTextButton}" />
                    <CustomControls:IconTextButtons
                        Width="200"
                        Height="136"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        Background="{StaticResource 9.png}"
